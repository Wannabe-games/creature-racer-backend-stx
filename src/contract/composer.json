{
    "type": "project",
    "version": "1.0.0",
    "license": "Private",
    "authors": [
        {
            "name": "MichaÅ‚ Wadas",
            "email": "michal@mklit.pl",
            "homepage": "http://www.mklit.pl/",
            "role": "Developer"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.2",
        "ext-ctype": "*",
        "ext-gmp": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "api-platform/core": "^2.5",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/mongodb-odm-bundle": "^4.4",
        "doctrine/orm": "^2.7",
        "endroid/qr-code": "^4.4",
        "gesdinet/jwt-refresh-token-bundle": "^0.9.0",
        "jms/serializer-bundle": "^3.0",
        "jms/translation-bundle": "^1.4",
        "kornrunner/keccak": "^1.1.0",
        "kornrunner/secp256k1": "^0.2.0",
        "lexik/jwt-authentication-bundle": "^2.14",
        "nelmio/cors-bundle": "^2.2",
        "predis/predis": "^1.1",
        "sensio/framework-extra-bundle": "^5.1",
        "simplito/elliptic-php": "^1.0",
        "snc/redis-bundle": "^4.1",
        "stripe/stripe-php": "^10.11",
        "symfony/asset": "^5.4",
        "symfony/cache": "^5.4",
        "symfony/console": "^5.4",
        "symfony/debug-bundle": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/expression-language": "^5.4",
        "symfony/flex": "^2",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/http-client": "^5.4",
        "symfony/monolog-bundle": "^3.2",
        "symfony/runtime": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/serializer": "^5.4",
        "symfony/translation": "^5.4",
        "symfony/twig-bridge": "^5.4",
        "symfony/uid": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/yaml": "^5.4",
        "twig/extra-bundle": "^3.3",
        "twig/intl-extra": "^3.3",
        "web3p/web3.php": "dev-master"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "App\\Entity\\": "../common/src/Entity/",
            "App\\Security\\": "../common/src/Security/",
            "App\\Doctrine\\": "../common/src/Doctrine/",
            "App\\Document\\": "../common/src/Document/",
            "App\\Common\\Service\\": "../common/src/Service/",
            "App\\Common\\Repository\\": "../common/src/Repository/",
            "App\\Common\\Exception\\": "../common/src/Exception/",
            "App\\Common\\Enum\\": "../common/src/Enum/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.4"
        },
        "runtime": {
            "dotenv_path": "../common/.env"
        }
    }
}
